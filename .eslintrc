{
  "extends": ["react-app", "eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-useless-constructor": 0,
    "import/no-extraneous-dependencies": 0,
    "import/named": 1,
    "import/no-unresolved": 1,
    "react/display-name": 0,
    "react/no-typos": 0,
    "no-console": 0
  },
  "plugins": ["prettier", "import", "react"]
}
